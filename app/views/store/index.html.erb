<h1></h1>

<h1>Categories:</h1>

<ul>
  <% @categories.each do |category| %>
    <li><%= link_to category.title, category_path(category) %><br></li>
  <% end %>
</ul>

<h1>Items:</h1>
<% @items.each do |item| %>
  <p><%= item.title %> | <%= "$#{item.price.to_f/100}" %> | <%= button_to 'Add to Cart', line_items_path(item_id: item) if user_signed_in?%></p>
<% end %>
